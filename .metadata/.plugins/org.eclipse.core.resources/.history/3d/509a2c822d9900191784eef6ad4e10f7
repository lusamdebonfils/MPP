package problem2.prob2B;

import java.util.HashMap;


public class Order {
	
	private int orderNum;
	private HashMap<String, Integer> existingOrderLines;

	public Order(int orderNum) {
		this.orderNum = orderNum;
		this.existingOrderLines = new HashMap<>();;
	}
	
	public void addOrderLine(String name, Double basePrice, int quantity) {
		OrderLine ord = new OrderLine(this,name,basePrice);
		existingOrderLines.put(ord, quantity);
	}

	public void getTotalCost() {
		double total = 0;
		
	}
	
	
}
